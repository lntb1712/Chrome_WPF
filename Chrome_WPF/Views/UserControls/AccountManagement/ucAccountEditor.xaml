<UserControl x:Class="Chrome_WPF.Views.UserControls.AccountManagement.ucAccountEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Chrome_WPF.Views.UserControls.AccountManagement"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="#F5F7FA">
        <StackPanel>
            <Button x:Name="BackButton" Content="Back" Click="BackButton_Click" Width="100" Height="30" HorizontalAlignment="Left" Margin="10"/>
            <!-- Các thành phần giao diện khác của ucAccountEditor -->
            <TextBlock Text="{Binding AccountName}" />
            <!-- Ví dụ hiển thị thông tin tài khoản -->
        </StackPanel>
    </Grid>
</UserControl>
